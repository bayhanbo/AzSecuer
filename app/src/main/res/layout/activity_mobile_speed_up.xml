<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.MobileSpeedUpActivity">
    <include
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_actionbar"
        android:layout_weight="0"
        layout="@layout/activity_home_include_actionbar"></include>
    <include
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_weight="0"
        layout="@layout/speedup_system"></include>
    <include layout="@layout/include_loading_list"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_weight="1"></include>
    <include
        android:id="@+id/speedup_include_checked_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        layout="@layout/speedup_checked_button"></include>
</LinearLayout>
