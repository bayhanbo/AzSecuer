<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="azsecuer.androidy.com.azsecuer.activity.FileManageActivity"
    >
    <TextView
        android:layout_width="@dimen/width_speed_ball"
        android:layout_height="@dimen/height_speed_ball"
        android:background="@drawable/home_score_pressed_bg"
        android:textSize="@dimen/size_actionbar_title"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:id="@+id/textView7" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:layout_marginTop="122dp"
        android:layout_marginLeft="225dp"
        android:id="@+id/textView8" />
    <Button
        android:id="@+id/bt_home_speed"
        android:text="@string/tv_bt_home_phoneSpeedup"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:textColor="@color/white"
        android:layout_alignBottom="@+id/textView7"
        android:background="@drawable/software_manager_room_bg"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="55dp" />
    <azsecuer.androidy.com.azsecuer.activity.view.CustomProc
        android:layout_width="240dp"
        android:layout_height="240dp"
        android:id="@+id/customPrco"
        app:roundColor="@color/background_gradient_end"
        app:roundProgressColor="@color/yellow"
        app:roundWidth="8dp"
        app:textColor="@color/black"
        app:textSize="35sp"
        app:max="100"
        app:style="STOKE"
        android:onClick="onClick"
        android:layout_marginTop="40dp"
        android:layout_centerHorizontal="true"
        />
</RelativeLayout>